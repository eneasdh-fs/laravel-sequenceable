{
    "name": "enea/sequenceable",
    "description": "Facilitates the generation and autocompletion of a sequential value in the database",
    "authors": [
        {
            "name": "enea dhack",
            "email": "enea.so@live.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "laravel/framework": "5.*"
    },

    "license": "MIT",
    "type": "library",

    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "orchestra/testbench": "~3.0",
        "orchestra/database": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Enea\\Sequenceable\\": "src/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Enea\\Tests\\": "tests/"
        }
    }


}
